@using WebApplication1.ViewModels;
@model PersonViewModel

<!DOCTYPE html>
<html>
<head>
    <title>Submit Result</title>
</head>
<body>
    <h2>Submitted Data</h2>

    <table border="1">
        <tr>
            <th>Name</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>FirstName</th>
            <td>@Model.FirstName</td>
        </tr>
        <tr>
            <th>LastName</th>
            <td>@Model.LastName</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Phone</th>
            <td>@Model.Phone</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>
        <tr>
            <th>City</th>
            <td>
                @foreach (var city in Model.City)
                {
                    <p>@city</p>
                }
            </td>
        </tr>
    </table>
</body>
</html>
